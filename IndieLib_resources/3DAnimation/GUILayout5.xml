<Panel visible     = "false"
       name        = "Main Frame">
  
  <Font path = "/NSGUI/ArialSmall.tga" />
  
  <Panel name        = "User Controls"
         layout      = "YAXIS"
         anchorPoint = "CORNERRU"> 
         
    <Position    x = "-10" y = "10"/>
    <Interval    y = "2" x = "2" />

    <TabbedPanel callbackString = "tp">
      <TabButtonsBordersColor  x = "50" g = "50" b = "50" />
      <TabButtonsColor         x = "255" g = "180" b = "40" />
      <BordersColor            x = "216" y = "169" z =  "0" />
      <BGColor                 x =  "50" y =  "50" z = "50" />

      <Panel name           = "Render"
             layout         = "YAXIS">



         <RadioButton callbackString = "face"
                   checked        = "false">
           <Text string = "face" />
         </RadioButton>
         <RadioButton callbackString = "Foot"
                   checked        = "false">
           <Text string = "Foot" />
         </RadioButton>
         <RadioButton callbackString = "untitled3ds"
                   checked        = "false">
           <Text string = "untitled3ds" />
         </RadioButton>
         <RadioButton callbackString = "untitledobj"
                   checked        = "false">
           <Text string = "untitledobj" />
         </RadioButton>
         <RadioButton callbackString = "watermesh"
                   checked        = "false">
           <Text string = "watermesh" />
         </RadioButton>
         <RadioButton callbackString = "cup"
                   checked        = "true">
           <Text string = "cup" />
         </RadioButton>

         <RadioButton callbackString = "cube3ds"
                   checked        = "false">
           <Text string = "cube3ds" />
         </RadioButton>

         <RadioButton callbackString = "gargoyle"
                   checked        = "false">
           <Text string = "gargoyle" />
         </RadioButton>

         <RadioButton callbackString = "cube"
                   checked        = "false">
           <Text string = "cube" />
         </RadioButton>

         <RadioButton callbackString = "sphere"
                   checked        = "false">
           <Text string = "sphere" />
         </RadioButton>
	<RadioButton callbackString = "teapot"
                   checked        = "false">
           <Text string = "teapot" />
         </RadioButton>
	



     </Panel>

    </TabbedPanel>
  </Panel>
</Panel>
