<Panel visible     = "false"
       name        = "Main Frame">
  
  <Font path = "/NSGUI/ArialSmall.tga" />
  
  <Panel name        = "User Controls"
         layout      = "YAXIS"
         anchorPoint = "CORNERRU"> 
         
    <Position    x = "-10" y = "10"/>
    <Interval    y = "2" x = "2" />

    <TabbedPanel callbackString = "tp">
      <TabButtonsBordersColor  x = "50" g = "50" b = "50" />
      <TabButtonsColor         x = "255" g = "180" b = "40" />
      <BordersColor            x = "216" y = "169" z =  "0" />
      <BGColor                 x =  "50" y =  "50" z = "50" />

      <Panel name           = "Render"
             layout         = "YAXIS"> 

         <RadioButton callbackString = "GoblinWalk"
                   checked        = "false">
           <Text string = "Goblin Walk" />
         </RadioButton>  

         <RadioButton callbackString = "RaptorWalk"
                   checked        = "false">
           <Text string = "Raptor Walk" />
         </RadioButton>
         
         <RadioButton callbackString = "RaptorEmpty"
                   checked        = "false">
           <Text string = "Raptor Empty" />
         </RadioButton>

         <RadioButton callbackString = "RaptorGungrab"
                   checked        = "false">
           <Text string = "Raptor Gungrab" />
         </RadioButton> 

         <RadioButton callbackString = "SpineBoyTest"
                   checked        = "false">
           <Text string = "SpineBoy Test" />
         </RadioButton>
         
         <RadioButton callbackString = "SpineBoyWalk"
                   checked        = "false">
           <Text string = "SpineBoy Walk" />
         </RadioButton>

         <RadioButton callbackString = "SpineBoyJump"
                   checked        = "false">
           <Text string = "SpineBoy Jump" />
         </RadioButton>

         <RadioButton callbackString = "SpineBoyRun"
                   checked        = "true">
           <Text string = "SpineBoy Run" />
         </RadioButton>
      </Panel>
  
    </TabbedPanel>
  </Panel>
</Panel>
