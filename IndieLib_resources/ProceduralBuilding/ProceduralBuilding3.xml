<Rules>
<Rule Name="Triangle" Body="  	Texture(5)
				Color(1, 0, 0)TexCoord(0.5, 1)Vertex( 0, 1, 0)
				Color(0, 1, 0)TexCoord(  0, 0)Vertex(-1,-1, 0)
				Color(0, 0, 1)TexCoord(  1, 0)Vertex( 1,-1, 0)" />
	
<Rule Name="Triangle" Body="	Texture(0) 
