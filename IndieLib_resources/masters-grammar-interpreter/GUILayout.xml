<Panel visible     = "false"
       name        = "Main Frame">
  
  <Font path = "/NSGUI/ArialSmall.tga" />
  
  <Panel name        = "User Controls"
         layout      = "YAXIS"
         anchorPoint = "CORNERRU"> 
         
    <Position    x = "-10" y = "10"/>
    <Interval    y = "2" x = "2" />

    <TabbedPanel callbackString = "tp">
      <TabButtonsBordersColor  x = "50" g = "50" b = "50" />
      <TabButtonsColor         x = "255" g = "180" b = "40" />
      <BordersColor            x = "216" y = "169" z =  "0" />
      <BGColor                 x =  "50" y =  "50" z = "50" />

      <Panel name           = "Render"
             layout         = "YAXIS">

         <RadioButton callbackString = "testing"
                   checked        = "false">
           <Text string = "testing" />
         </RadioButton>
         <RadioButton callbackString = "borg_cube"
                   checked        = "false">
           <Text string = "borg_cube" />
         </RadioButton>
         <RadioButton callbackString = "c_station"
                   checked        = "false">
