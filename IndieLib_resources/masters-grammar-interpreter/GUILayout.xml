         </RadioButton>
         <RadioButton callbackString = "enterprise"
                   checked        = "false">
           <Text string = "enterprise" />
         </RadioButton>
         <RadioButton callbackString = "klingon"
                   checked        = "false">
           <Text string = "klingon" />
         </RadioButton>
         <RadioButton callbackString = "rotational_ship"
                   checked        = "false">
           <Text string = "rotational_ship" />
         </RadioButton>
         <RadioButton callbackString = "space_station"
                   checked        = "false">
           <Text string = "space_station" />
         </RadioButton>
         <RadioButton callbackString = "spike_ship"
                   checked        = "false">
           <Text string = "spike_ship" />
         </RadioButton>
         <RadioButton callbackString = "squid_ship"
                   checked        = "false">
           <Text string = "squid_ship" />
         </RadioButton>
         <RadioButton callbackString = "tie_fighter"
                   checked        = "true">
           <Text string = "tie_fighter" />
         </RadioButton>
         <RadioButton callbackString = "wing_ship"
                   checked        = "false">
           <Text string = "wing_ship" />
         </RadioButton>

     </Panel>

