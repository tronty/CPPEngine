[Fragment shader]

<compile ps_2_0 main>

sampler sampSourceImage;

//--------------------------------------------------------------------------------------
// Simple shader to copy texture
//--------------------------------------------------------------------------------------
float4 main( float2 Tex : TEXCOORD0 ) : COLOR0
{
    return tex2D(sampSourceImage, Tex);
}
